# Copy this file to terraform.tfvars and fill in your values
# DO NOT commit terraform.tfvars to git (it's in .gitignore)

# Azure subscription ID (required)
# subscription_id = "your-subscription-id-here"

# For service principal authentication (production)
# tenant_id     = "your-tenant-id"
# client_id     = "your-client-id"  
# client_secret = "your-client-secret"

# Project configuration (optional - defaults will be used if not specified)
# resource_group_name = "rg-terraform-react"
# location           = "West Europe"
# app_name_prefix    = "terraform-react-app"
# environment        = "dev"